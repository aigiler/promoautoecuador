<odoo>
    <data>
        <template id="template_bank_statement_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page" style="font-size: 14px">
                    <span><center><h1>Conciliacion Bancaria</h1></center></span><br/><br/>
                    <span><strong>Corte: </strong></span> <span t-esc="o.date_reporte"/><br/>
                    <span><strong>Cuenta: </strong></span> <span t-esc="o.journal_id.name"/><br/>
                    <span><strong>Saldo Estado de cuenta Inicial </strong></span> <span t-esc="o.saldo_cuenta"/><br/>
                    <span><strong>Diferencia </strong></span> <span t-esc="o.saldo_cuenta - o.diferencia"/><br/>
                    <span><strong>Cheques No Cobrados </strong></span> <span t-esc="o.subtotal_cheques_no_cobrados"/><br/>
                    <span><strong>Notas Débito No Incluídas </strong></span> <span t-esc="o.subtotal_debitos_no_cobrados"/><br/>
                    <span><strong>Depósitos No Incluídos </strong></span> <span t-esc="o.subtotal_depositos_no_cobrados"/><br/>
                    <span><strong>Créditos No Incluídos </strong></span> <span t-esc="o.subtotal_creditos_no_cobrados"/><br/>


                    <span><strong>Notas Débito (No Contabilizadas)</strong></span> <span t-esc="o.subtotal_cheques_no_cobrados_no_cont"/><br/>
                    <span><strong>Cheques No Contabilizados </strong></span> <span t-esc="o.subtotal_debitos_no_cobrados_no_cont"/><br/>
                    <span><strong>Notas Crédito (No Contabilizadas)</strong></span> <span t-esc="o.subtotal_depositos_no_cobrados_no_cont"/><br/>
                    <span><strong>Depósitos No Contabilizado</strong></span> <span t-esc="o.subtotal_creditos_no_cobrados_no_cont"/><br/>






                    <div class="clearfix" name="so_total_summary">
                        <div id="total" class="row" name="total">
                            <div t-attf-class="#{'col-4' if report_type != 'html' else 'col-sm-7 col-md-5'} ml-auto">
                                <table class="table table-sm">

                                    <tr class="border-black o_total">
                                        <td ><strong>SALDO SEGUN ESTADO DE CUENTA</strong></td>
                                        <td  class="text-right">
                                            <span t-esc="o.saldo_cuenta"/>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>







                    <span><strong>(+) Depositos No Incluidos</strong></span><br/>
                    <table class="table table-condensed">
                    <thead style="text-align: center; margin-bottom: 10px ">
                        <th>Nro. Deposito</th>
                        <th>Fec. Deposito</th>
                        <th>Empresa</th>
                        <th>Descripcion</th>
                        <th>Monto</th>

                    </thead>
                    <tbody>
                    <tr t-foreach="o.body_report('deposito')" t-as="reconciled">
                        <td><span t-esc="reconciled.numero_documento" /></td>
                        <td><span t-esc="reconciled.fecha_emision" /></td>
                        <td><span t-esc="reconciled.empresa" /></td>
                        <td><span t-esc="reconciled.referencia" /></td>
                        <td style="text-align: right"><span t-esc="'%.2f' %(reconciled.monto)" /></td>
                    </tr>
                    </tbody>


                    </table>


                    <div class="clearfix" name="so_total_summary">
                        <div id="total" class="row" name="total">
                            <div t-attf-class="#{'col-4' if report_type != 'html' else 'col-sm-7 col-md-5'} ml-auto">
                                <table class="table table-sm">

                                    <tr class="border-black o_total">
                                        <td ><strong>SubTotal  (+) Depositos No Incluidos </strong></td>
                                        <td  class="text-right">
                                            <span t-esc="o.subtotal_depositos_no_cobrados"/>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>






                    <span><strong>(+) Creditos No Incluidos</strong></span><br/>
