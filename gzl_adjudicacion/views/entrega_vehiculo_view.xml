                        <separator/>
                        <separator class="text-center" string="OBSERVACIONES"/>
                        <div class="row">
                            <field name="observaciones" widget="pad" string='Referencias indican:'/>
                        </div>
                        <separator/>
                        <separator class="text-center" string="COMITÉ DE APROBACION"/>
                        <div class="row">
                        </div>
                    </div>
