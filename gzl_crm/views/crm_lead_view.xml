<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_crm_lead_inherit_form" model="ir.ui.view">
        <field name="name">view.crm.lead.inherit.form</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <field name="is_won" invisible="1"/>
                <button name="crear_adjudicado" class="oe_highlight" string="Crear Adjudicado" type="object"  attrs="{'invisible': [('is_won', '=',False)]}"/>
            </xpath>
        </field>
    </record>

</odoo>